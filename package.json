{
  "name": "shipping-protection",
  "version": "0.0.0",
  "description": "Narvar Shipping Protection SDK",
  "type": "module",
  "main": "./dist/shipping-protection.js",
  "module": "./dist/shipping-protection.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run postbuild",
    "postbuild": "mkdir -p public/dist && cp dist/shipping-protection.js public/dist/",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "size": "size-limit",
    "check": "npm run test && npm run size"
  },
  "keywords": [
    "narvar",
    "shipping-protection",
    "lit",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "sideEffects": [
    "src/components/**/*.ts",
    "src/demo/**/*.ts",
    "src/index.ts"
  ],
  "size-limit": [
    {
      "path": "dist/shipping-protection.js",
      "limit": "60 KB"
    }
  ],
  "dependencies": {
    "lit": "^3.3.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.14",
    "jsdom": "^27.2.0",
    "size-limit": "^11.2.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
}
